+	}
+
+	/**
+	 * @return The retrieved Integer List
+	 */
+	public NBTList<Integer> getIntegerList(String name) {
+		try {
+			writeLock.lock();
+			NBTList<Integer> list = NBTReflectionUtil.getList(this, name, NBTType.NBTTagInt, Integer.class);
+			saveCompound();
