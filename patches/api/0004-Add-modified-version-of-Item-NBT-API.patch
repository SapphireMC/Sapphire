+	public static NBTContainer convertNMSItemToNBTCompound(Object nmsitem) {
+		try {
+			Object answer = ReflectionMethod.NMSITEM_SAVE.run(nmsitem, ObjectCreator.NMS_NBTTAGCOMPOUND.getInstance());
+			return new NBTContainer(answer);
