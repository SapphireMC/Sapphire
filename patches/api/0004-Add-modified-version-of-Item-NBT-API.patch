+	}
+
+	/**
+	 * @return The retrieved Float List
+	 */
+	public NBTList<Float> getFloatList(String name) {
+		try {
+			writeLock.lock();
+			NBTList<Float> list = NBTReflectionUtil.getList(this, name, NBTType.NBTTagFloat, Float.class);
