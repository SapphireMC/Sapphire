+	public static Object getItemRootNBTTagCompound(Object nmsItem) {
+		try {
+			Object answer = ReflectionMethod.NMSITEM_GETTAG.run(nmsItem);
+			return answer != null ? answer : ObjectCreator.NMS_NBTTAGCOMPOUND.getInstance();
